<div id="form-main">
  <div id="form-div">
    <%= form_tag(sessions_path) do%>
      <p class="name">
        Name: <%= text_field_tag 'user[name]', class: "validate[required,custom[onlyLetter],length[0,100]] feedback-input" %>
      </p>
      Password: <%= password_field_tag 'user[password]' %>

      <div class="submit">
        <%= submit_tag "Sign In", id: "button-blue"%>
      </div>
    <% end %>
  </form>
</div>

